# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

source 'https://github.com/artsy/Specs.git'
source 'https://github.com/CocoaPods/Specs.git'
source 'https://github.com/brightcove/BrightcoveSpecs.git'

target 'GSSHOP' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for GSSHOP
pod 'AirBridge', '= 1.12.0'
pod 'Appboy-iOS-SDK', '= 3.21.2' #'= 3.8.4' xcode9
pod 'Amplitude-iOS', '= 4.3.1'  #'= 3.14.1'
pod 'Socket.IO-Client-Swift', '= 13.3.1'    #'= 13.3.0'
pod 'Brightcove-Player-Core', '= 6.3.9'
pod 'Starscream', '= 3.0.5'
pod 'AlamofireObjectMapper', '~> 5.2'           # Swift Network & Object Mapper
pod 'FacebookSDK', '~> 5.3.0'
pod 'Apptimize', '= 3.3.0'          # lastest version
pod 'GoogleTagManager', '~> 6.0'
pod 'Firebase/Analytics'
pod 'KakaoOpenSDK', '= 1.21.0'
pod 'SVGKit', :git => 'https://github.com/SVGKit/SVGKit.git', :branch => '3.x'
pod 'lottie-ios', '= 2.5.3'
pod 'naveridlogin-sdk-ios', '~> 4.1.2'
pod 'BSImagePicker', '~> 2.9.0'

end

